-static void GetColorsFromRGBA(uint32_t rgba, float* r, float* g, float* b, float* a) {
-  *b = (rgba & 0xFF) / 255.0;
-  *g = ((rgba & 0xFF00) >> 8) / 255.0;
-  *r = ((rgba & 0xFF0000) >> 16) / 255.0;
-  *a = ((rgba & 0xFF000000) >> 24) / 255.0;
-}
