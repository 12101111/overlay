-int32_t pluginGetClipRegionRectCount(InstanceData* instanceData) {
-  RGNDATA* data = computeClipRegion(instanceData);
-  if (!data) return NPTEST_INT32_ERROR;
-
-  int32_t result = data->rdh.nCount;
-  ::HeapFree(::GetProcessHeap(), 0, data);
-  return result;
-}
