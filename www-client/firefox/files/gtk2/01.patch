-static BOOL getWindowRegion(HWND wnd, HRGN rgn) {
-  if (::GetWindowRgn(wnd, rgn) != ERROR) return TRUE;
-
-  RECT clientRect;
-  if (!::GetClientRect(wnd, &clientRect)) return FALSE;
-  return ::SetRectRgn(rgn, 0, 0, clientRect.right, clientRect.bottom);
-}
