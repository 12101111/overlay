From 12cac3a31b72216affb695f1445f87de5eba71b1 Mon Sep 17 00:00:00 2001
From: 12101111 <w12101111@gmail.com>
Date: Fri, 8 May 2020 20:46:40 +0800
Subject: [PATCH 05/10] Allow build using nodejs-14

---
 build/npm/preinstall.js | 4 ++--
 1 file changed, 2 insertions(+), 2 deletions(-)

diff --git a/build/npm/preinstall.js b/build/npm/preinstall.js
index cb88d37ade..b0bbb96341 100644
--- a/build/npm/preinstall.js
+++ b/build/npm/preinstall.js
@@ -7,8 +7,8 @@ let err = false;
 
 const majorNodeVersion = parseInt(/^(\d+)\./.exec(process.versions.node)[1]);
 
-if (majorNodeVersion < 10 || majorNodeVersion >= 13) {
-	console.error('\033[1;31m*** Please use node >=10 and <=12.\033[0;0m');
+if (majorNodeVersion < 10 || majorNodeVersion >= 15) {
+	console.error('\033[1;31m*** Please use node >=10 and <=14.\033[0;0m');
 	err = true;
 }
 
