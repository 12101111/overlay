npm_uris() {
	local -r regex='^https\:\/\/registry.npmjs.org\/(.+)\/\-\/([^/]+).tgz$'
	local -r namespace='^@([^@/]+)\/([^@/]+)$'
	local uri
	for uri in "$@"; do
		local name filename
		[[ $uri =~ $regex ]] || die "Could not parse url: ${uri}"
			name="${BASH_REMATCH[1]}"
			filename="${BASH_REMATCH[2]}"   
		if [[ $name =~ $namespace ]]; then
			local scope
			scope="${BASH_REMATCH[1]}"
			echo "${uri} -> @${scope}-${filename}.tgz"
		else
			echo "${uri} -> ${filename}.tgz"
		fi
	done
}

