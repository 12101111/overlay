	local npm_path="/usr/$(get_libdir)/node_modules/npm/"
	local node_gyp_path="${npm_path}bin/node-gyp-bin"
	if [ -f "${node-gyp_path}/node-gyp" ]; then
		export PATH="${node_gyp_path}:${PATH}"
	else
		create_fake_bin "node ${npm_path}node_modules/node-gyp/bin/node-gyp.js" node-gyp
		export PATH="${S}:${PATH}"
	fi

	# Build native modules for system electron
	local electron_version="$(get_local_electron_version)"
	einfo "Build against Electron ${electron_version}"
	sed -i "s/^target=\".*\"/target=\"${electron_version//v/}\"/" "${S}"/.npmrc || die

	# use offline cache
	npm config set offline true || die

	# move ripgrep tarball to cache directory/usr/lib/node_modules/npm/bin/node-gyp-bin/node-gyp
	# https://github.com/microsoft/vscode-ripgrep/blob/master/lib/download.js#L14
	pushd "${WORKDIR}" >/dev/null || die
	local source_rg_tar_path="${DISTDIR}/$(get_rg_tar_name 0)"
	if use system-ripgrep; then
		create_fake_bin /usr/bin/rg rg
		tar cf "$(get_rg_tar_name 0)" rg  || die
		source_rg_tar_path="${WORKDIR}/$(get_rg_tar_name 0)"
	fi
	for i in {0..1}; do
		local rg_cache_path="${TMPDIR}/vscode-ripgrep-cache-${VSCODE_RIPGREP_VERSION[$i]}"
		mkdir -p "${rg_cache_path}" || die
		cp "${source_rg_tar_path}" "${rg_cache_path}/"
	done
	popd >/dev/null || die

	# Failed to build against nodejs 20, only needed by win32
	sed -i '/tree-sitter"/d' build/package.json || die

	# GIT dependency can't fetch via cache
	cp "${DISTDIR}/${CSS_PARSER_COMMIT}" "${TMPDIR}/css-parser.tgz"
	sed -i "s|\"@emmetio/css-parser\": \"ramya-rao-a/css-parser#vscode\",|\"@emmetio/css-parser\": \"file:/${TMPDIR}/css-parser.tgz\",|" \
		extensions/emmet/package.json || die
	pushd "${S}"/extensions/emmet || die
	npm install --package-lock-only --no-progress --no-audit --verbose --nodedir="$(get_electron_nodedir)" || die
	popd >/dev/null || die

	sed -i "/execSync/d" "${S}/node-pty/scripts/post-install.js" || die
	tar cf node-pty.tar.gz node-pty || die
	# don't fetch node-gyp
	sed -i "s|\"node-pty\": \"\^1.1.0-beta33\",|\"node-pty\": \"file:/${S}/node-pty.tar.gz\",|" package.json || die
	sed -i "s|\"node-pty\": \"\^1.1.0-beta33\",|\"node-pty\": \"file:/${S}/node-pty.tar.gz\",|" remote/package.json || die

	pushd "${S}/vscode-spdlog" >/dev/null || die
	eapply "${FILESDIR}/fix-spdlog.patch"
	popd >/dev/null || die
	tar cf vscode-spdlog.tar.gz vscode-spdlog || die
	sed -i "s|\"\@vscode\/spdlog\": \"\^0.15.2\",|\"\@vscode\/spdlog\": \"file:/${S}/vscode-spdlog.tar.gz\",|" package.json || die
	sed -i "s|\"\@vscode\/spdlog\": \"\^0.15.2\",|\"\@vscode\/spdlog\": \"file:/${S}/vscode-spdlog.tar.gz\",|" remote/package.json || die

	restore_config product.json
}

src_configure() {
	export ELECTRON_SKIP_BINARY_DOWNLOAD=1
	export PLAYWRIGHT_SKIP_BROWSER_DOWNLOAD=1
	export PREFIX=${TMPDIR}
	export DESTDIR=${TMPDIR}
	local DEPS=$(ls "${DISTDIR}"/*.tgz 2>/dev/null)
	npm cache add $DEPS --no-progress --verbose || die
	npm ci --offline --no-progress --no-audit --verbose --nodedir="$(get_electron_nodedir)" || die
}

src_compile() {
	export BUILD_SOURCEVERSION=${COMMIT}

	npm run gulp vscode-linux-$(get_arch)-min || die
}

src_install() {
	export DESTDIR=${ED}
	export LIBDIR=$(get_libdir)
	export XDG_CACHE_HOME=${TMPDIR}
	npm run gulp vscode-linux-$(get_arch)-install-gentoo || die

	local vscode_path="/usr/$(get_libdir)/vscode"
	local app_name="$(ls ${ED}${vscode_path}/bin)"

	if use builtin-extensions; then
		for k in "${!BUILTINEXTS[@]}"; do
			zcat "${DISTDIR}/ms-vscode.${k}-${BUILTINEXTS[${k}]}.zip.gz" > "${TMPDIR}/ms-vscode.${k}-${BUILTINEXTS[${k}]}.zip"
			unzip "${TMPDIR}/ms-vscode.${k}-${BUILTINEXTS[${k}]}.zip" "extension/*" -d "${WORKDIR}"
			mv "${WORKDIR}/extension" "${ED}${vscode_path}/resources/app/extensions/ms-vscode.${k}"
		done
	fi

	sed -i "2i	\"commit\": \"${COMMIT}\"," "${ED}${vscode_path}/resources/app/product.json"
	sed -i "s/ELECTRON=\"\$VSCODE_PATH\/${app_name}\"/ELECTRON=\"\/usr\/$(get_libdir)\/electron-${ELECTRON_SLOT}\/electron\"/g" \
		"${ED}${vscode_path}/bin/${app_name}"

	if use system-ripgrep; then
		# old path with asar enabled
		#local ripgrep_path="${ED}${vscode_path}/resources/app/node_modules.asar.unpacked/@vscode/ripgrep/bin/rg"
		local ripgrep_path="${ED}${vscode_path}/resources/app/node_modules/@vscode/ripgrep/bin/rg"
		rm "${ripgrep_path}" || die
		ln -s /usr/bin/rg "${ripgrep_path}" || die
	fi

	dosym ${vscode_path}/bin/${app_name} /usr/bin/${app_name}
	save_config product.json
}

pkg_postinst() {
	xdg_pkg_postinst

	if ! use builtin-extensions; then
		elog "Builtin extensions is disabled and not downloaded."
		elog "If you want to use them, you can download from Visual Studio Marketplace"
		elog "The list of builtin extensions:"
		for k in "${!BUILTINEXTS[@]}"; do
			elog "ms-vscode.${k}-${BUILTINEXTS[${k}]}"
		done
	fi
}

get_electron_dir() {
	echo "/usr/$(get_libdir)/electron-${ELECTRON_SLOT}"
}

get_local_electron_version() {
	echo "$(<$(get_electron_dir)/version)"
}

get_electron_nodedir() {
	echo "/usr/include/electron-${ELECTRON_SLOT}/node"
}

get_arch() {
	case "${ARCH}:${ABI}" in
	  *:amd64) echo "x64" ;;
	  *:arm) echo "arm" ;;
	  *:arm64) echo "arm64" ;;
	  loong:lp64*) echo "loong64" ;;
	  riscv:lp64*) echo "riscv64" ;;
	  *) die "${ARCH} ${ABI} not supported";;
	esac
}

get_rg_tar_name() {
	local myarch=""
	case ${ABI} in
	amd64) myarch="x86_64-unknown-linux-musl" ;;
	arm) myarch="arm-unknown-linux-gnueabihf" ;;
	arm64) myarch="aarch64-unknown-linux-musl";;
	ppc64) myarch="powerpc64le-unknown-linux-gnu" ;;
	# note loongarch use this file name
	*) myarch="i686-unknown-linux-musl" ;;
	esac
	echo "ripgrep-v${RG_VERSION[$1]}-${myarch}.tar.gz"
}

create_fake_bin() {
	echo "#!/bin/sh" >"$2"
	echo "${1} \$@" >>"$2"
	chmod +x "$2"
}
